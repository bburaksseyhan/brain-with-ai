{
    "version": 2,
    "builds": [
        {
            "src": "**/*.js",
            "use": "@vercel/static"
        },
        {
            "src": "**/*.html",
            "use": "@vercel/static"
        },
        {
            "src": "**/*.css",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/brain-regions.js",
            "dest": "/brain-regions.js"
        },
        {
            "src": "/main.js",
            "dest": "/main.js"
        },
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
} 